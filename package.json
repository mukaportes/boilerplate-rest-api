{
  "name": "boilerplate-rest-api",
  "version": "1.0.0",
  "description": "Personal REST API boilerplate for Node.js",
  "main": "index.js",
  "scripts": {
    "db:migrate": "knex --knexfile ../src/app/config/knexfile.js migrate:latest",
    "db:rollback": "knex --knexfile ../src/app/config/knexfile.js migrate:rollback",
    "start": "nodemon src/index.js",
    "test": "jest --config test.json --forceExit --collectCoverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mukaportes/boilerplate-rest-api.git"
  },
  "author": "Murilo Portescheller <@mukaportes>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/mukaportes/boilerplate-rest-api/issues"
  },
  "homepage": "https://github.com/mukaportes/boilerplate-rest-api#readme",
  "devDependencies": {
    "eslint": "^7.3.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "faker": "^4.1.0",
    "jest": "^26.1.0"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "knex": "^0.21.1",
    "moment": "^2.27.0",
    "nodemon": "^2.0.4",
    "pg": "^8.2.1",
    "sequelize": "^6.2.0",
    "winston": "^3.3.3"
  }
}
